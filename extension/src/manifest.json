{
  "name": "My Reading List",
  "version": "1.0",
  "description": "Tag and Search your reading list with ease",
  "permissions": [
    "tabs",
    "storage"
  ],
  "omnibox": { "keyword" : "be" },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
